/**
 * Interface class for handling option calls 
 * @author Tristan Fazio
 * 
 */

 public interface Option
 {
    //label for the option 
    private int label;
    //default constructor
    public Option(int i)
    {
        label=i;
    }
    //abstract option call
    String doOption()
    //get label
    public int getLabel()
    {
        return label;
    }
 }